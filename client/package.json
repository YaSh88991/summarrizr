    "babel-jest": "^30.0.4",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4"
  },

   "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|mp4|svg)$": "<rootDir>/src/__mocks__/fileMock.js"
    },
    "transform": {
      "^.+\\.[jt]sx?$": "babel-jest"
    }
  }
